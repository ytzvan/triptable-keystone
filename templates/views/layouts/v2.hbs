<!DOCTYPE html>
<head>
<!-- Basic Page Needs
================================================== -->
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
{{#if meta.title}}
	<title>{{meta.title}}</title>
	{{else}}
	<title>{{i18n "title"}}</title>
	{{/if}}
	<meta name="author" content="Triptable">
	{{#if meta.keywords}}
	<meta name="keywords" content="tours, experiencias, excursiones, reserva, cosas que hacer, trips, {{meta.keywords}}" />
	{{else}}
	<meta name="keywords" content="tours en latinoamerica, tours, excursiones, que hacer, booking, trips, travel, turismo, latinoamerica" />
	{{/if}}
	{{#if meta.description}}
	<meta name="description" content="{{meta.description}}">
	{{else}}
	<meta name="description" content="{{i18n "meta_description"}}">
	{{/if}}
	<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
	{{#if meta.ogTitle}}
	<meta property="og:title" content="{{meta.ogTitle}}"/>
	{{else}}
	<meta property="og:title" content="Triptable: Reserva Tours y Actividades en Panamá."/>
	{{/if}}
	{{#if meta.ogDescription}}
	<meta property="og:description" content="{{meta.ogDescription}}" />
	{{else}}
	<meta property="og:description" content="Descubre y reserva tours, actividades, viajes y turismo en Panama. Con Triptable reservas experiencias locales unicas." />
	{{/if}}
	{{#if meta.url}}
	<meta property="og:url" content="{{meta.url}}"/>
  <link rel="canonical" href="{{meta.url}}" />
	{{else}}
	<meta property="og:url" content="https://www.triptable.com"/>
  <link rel="canonical" href="https://www.triptable.com" />
	{{/if}}
	<meta property="og:site_name" content="Triptable: Reserva Tours y Actividades"/>
  {{#if meta.ogType}}
  <meta property="og:type" content="{{meta.ogType}}"/>
  {{else}}
	<meta property="og:type" content="website"/>
  {{/if}}
	{{#if meta.image}}
	<meta property="og:image" content="{{meta.image}}" />
	<meta property="og:image:secure_url"  content="{{meta.image}}" />
	{{else}}
	<meta property="og:image" content="https://res.cloudinary.com/triptable/image/upload/c_fill,q_60,w_1440/v1460674515/01_tqumqz.jpg" />
	<meta property="og:image:secure_url" content="https://res.cloudinary.com/triptable/image/upload/c_fill,q_60,w_1440/v1460674515/01_tqumqz.jpg" />
	{{/if}}
