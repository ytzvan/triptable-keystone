	{{else}}
	<meta property="og:description" content="Descubre y reserva tours, actividades, viajes y turismo en Panama. Con Triptable reservas experiencias locales unicas." />
	{{/if}}
	<meta property="og:site_name" content="Triptable: Reserva Tours y Actividades"/>
