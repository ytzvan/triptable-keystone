			<script>
				$(document).ready(function(){
					var lang= "{{lang}}";
					if (lang == "en"){
						var str = '<i class="flag-icon flag-icon-gb">';
						$('#lang-select').html(str);
					} else {
						var str = '<i class="flag-icon flag-icon-{{lang}}">';
						$('#lang-select').html(str);
					};
					var currency =  "{{data.currency}}";
					
					var simbol = getSymbol(currency);
					var finalStr = simbol + " " +  currency;
					$('#currency-select').html(finalStr);

					
				//	$('#currency-select').HTML(currency);
					

					function getSymbol(currency) {
						var symbol;
						if (currency == "EUR"){
							var symbol = "€";
						} else if (currency == "CNY"){
							var symbol = "¥";
						} else if (currency =="BRL"){
							var symbol = "R$";
						} else {
							var symbol = "$";
						}
						return symbol;
					}
				});

			
			</script>

