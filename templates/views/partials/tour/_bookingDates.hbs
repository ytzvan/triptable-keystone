
 <div class='input-group date col-sm-5 col-md-4' id='datetimepicker11' style="display:none;">
   <input type='text' required name="date" id="date" class="form-control" />
   <span class="input-group-addon">
     <span class="glyphicon glyphicon-calendar">
     </span>
   </span>
</div>
<div id="section-5" class="detail-content">
  <div class="section-title text-left" style="margin:0;">
    <h4>Escoge un Tour</h4>
  </div>
  <div class="row">
    <div class="col-sm-6">
      <div class="form-group form-icon">
    </div>
  </div>
</div>
<div class="availabily-wrapper">
  <ul class="availabily-list">
    <li class="availabily-heading clearfix">
      <div class="date-from"></div>
      <div class="date-to"></div>
      <div class="status"></div>
      <div class="price"></div>
      <div class="action">&nbsp;</div>
    </li>
      <li class="availabily-content clearfix">
      <div class="date-from" style="width: 50%;">
        <span class="availabily-heading-label">Tour:</span>Tour:<span> {{data.tour.name}}</span>
      </div>
      <div class="price" style="width: 15%;">
        <span class="availabily-heading-label">Duración:</span>Duración:
        <span id="price">{{data.tour.duration}} {{#if data.tour.isMultiDay}} Dias {{else}} Horas {{/if}}</span>
      </div>
       <div class="price" style="width: 15%;">
        <span class="availabily-heading-label">Precio por Persona:</span>Precio por Persona:
        <span id="price">{{data.tour.price}}$</span>
      </div>
      <div class="action" style="width: 15%;">
        <a href="/contact/{{data.tour.tourId}}" id="bookingBtn{{@index}}" class="btn btn-primary btn-sm btn-inverse">Reservar</a>
      </div>
    </li>
    {{#each data.tour.childs}}
    <li class="availabily-content clearfix">
      <div class="date-from" style="width: 50%;">
        <span class="availabily-heading-label">Tour:</span>Tour:<span> {{name}}</span>
      </div>
      <div class="price" style="width: 15%;">
        <span class="availabily-heading-label">Duración:</span>Duración:
        <span id="price">{{duration}} {{#if isMultiDay}} Dias {{else}} Horas {{/if}}</span>
      </div>
       <div class="price" style="width: 15%;">
        <span class="availabily-heading-label">Precio por Persona:</span>Precio por Persona:
        <span id="price{{@index}}">{{price}}$</span>
      </div>
      <div class="action" style="width: 15%;">
        <a href="/contact/{{tourId}}" id="bookingBtn{{@index}}" class="btn btn-primary btn-sm btn-inverse">Reservar</a>
      </div>
    </li>
    {{/each}}


  </ul>
</div>
